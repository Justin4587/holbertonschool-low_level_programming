#include "dog.h"
#include <stdio.h>

/**
* new_dog - print compile file
* description: dog stuff
* @name: name of pet
* @age: age
* @owner: name of owner
* Return: NULL if fail
*/

dog_t *new_dog(char *name, float age, char *owner)
{
  int n;
  int o;

  for (n = 0; name[n]; n++);
  for (o = 0; owner[o]; o++);

  new_dog = malloc(sizeof(dog_t) + sizeof(char) * n + o + 1);

}
