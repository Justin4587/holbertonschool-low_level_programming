#include <stdio.h>
#include <stdlib.h>
#include <time.h>

/**
* main - time determined variable number output
* description: if then program
* Return: 0
*/

int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	printf("Last digit of\n%d\nis% 10")
if (% 10  > 5)
{
printf("and is greater than 5\n", n);
}
if (% 10  == 0)
{
	printf("and is 0\n", n);
	}
 if ((% 10 < 6)&&(% 10 != 0)) 
{
	printf("and is less than 6 but not 0\n", n);
	}
	return (0);
}
