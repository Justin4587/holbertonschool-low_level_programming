#include <stdio.h>
#include <stdlib.h>
#include <time.h>

/**
* main - time determined variable number output
* description: if then program
* Return: 0
*/

/**
* function main - return variable value
* check size of int
*/

int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
if (n > 0)
{
printf("%d is possitive\n", n);
}
if (n == 0)
{
	printf("%d is zero\n", n);
	}
if (n < 0)
{
	printf("%d is negative\n", n);
	}
	return (0);
}
