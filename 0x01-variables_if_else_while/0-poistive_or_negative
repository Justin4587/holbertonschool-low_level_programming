#include <stdio.h>
#include <stdlib.h>
#include <time.h>

/**
* main - time determined variable number output
* description: if then program
* Return: 0
*/

/** 
* function main - return variable value 
* check size of int
*/

int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	
	if (n>0) {
	printf("is possitive");
	}
	if (n=0) {
	printf("is zero"); 
	}
	if (n<0) {
	printf("is negative");
	}
	return (0);
}