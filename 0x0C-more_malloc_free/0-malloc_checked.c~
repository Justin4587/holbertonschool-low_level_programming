#include "holberton.h"
#include <stdlib.h>

/**
* **alloc_grid - make 2d grid
* description: make 2d array
* @width: int
* @height: int
* Return: value or error
*/

void *malloc_checked(unsigned int b)
{
  void *ptr;

  ptr = malloc(b);
  if (ptr == NULL)
    {
    free(ptr);
  exit(98);
    }
  return (ptr);
}
